{
  "name": "codenames-arabic-word-pack",
  "version": "1.0.0",
  "description": "Codenames custom word pack in Arabic",
  "main": "src/app.js",
  "scripts": {
    "build-0": "cd client && yarn build",
    "build": "npx tsc && netlify-lambda build dist",
    "dev-0": "concurrently \"yarn start\" \"yarn build\"",
    "dev": "ts-node src/app.ts",
    "start": "netlify-lambda serve dist",
    "postinstall": "cd client && yarn install"
  },
  "repository": "https://github.com/adnanlah/codenames-arabic-word-pack.git",
  "author": "Adnan Lahrech <lahadn@gmail.com>",
  "dependencies": {
    "body-parser": "^1.19.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.2"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/supertest": "^2.0.11",
    "concurrently": "^7.0.0",
    "jest": "^27.5.1",
    "netlify-lambda": "^2.0.15",
    "serve": "^13.0.2",
    "serverless-http": "^2.7.0",
    "shelljs": "^0.8.5",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  }
}
